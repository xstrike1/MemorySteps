{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:61160",
      "sslPort": 44340
    }
  },
  "profiles": {
    "Electron.NET App": {
      "commandName": "Executable",
